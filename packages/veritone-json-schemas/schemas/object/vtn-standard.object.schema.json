{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://veritone.com/vtn-standard.object.schema.json",
  "title": "vtn-standard.object",
  "description": "Standard engine output for Object at Veritone",
  "type": "object",
  "definitions": {
    "objectResult": {
      "$ref": "vtn-standard.schema.json#/definitions/objectResult"
    }
  },
  "properties": {
    "object": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/objectResult"
      }
    },
    "series": {
      "type": "array",
      "items": {
        "type": "object",
        "allOf": [
          {
            "properties": {
              "object": {
                "$ref": "#/definitions/objectResult"
              },
              "startTimeMs": {
                "type": "integer",
                "minimum": 0
              },
              "stopTimeMs": {
                "type": "integer",
                "minimum": 0
              }
            },
            "required": ["startTimeMs", "stopTimeMs"]
          }
        ]
      }
    }
  }
}
