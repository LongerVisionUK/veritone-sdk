{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://docs.veritone.com/schemas/vtn-standard/object.json",
  "title": "vtn-standard.object",
  "description": "Standard engine output for Object at Veritone",
  "type": "object",
  "definitions": {
    "objectResult": {
      "type": "object",
      "allOf": [
        {
          "$ref": "master.json#/definitions/objectResult"
        },
        {
          "properties": {
            "type": {
              "const": "object"
            }
          }
        }
      ],
      "required": [
        "type"
      ]
    }
  },
  "properties": {
    "object": {
      "type": "array",
      "items": {
        "type": "object",
        "allOf": [
          {
            "properties": {
              "object": {
                "$ref": "#/definitions/objectResult"
              }
            }
          }
        ]
      }
    },
    "series": {
      "type": "array",
      "items": {
        "type": "object",
        "allOf": [
          {
            "properties": {
              "object": {
                "$ref": "#/definitions/objectResult"
              },
              "startTimeMs": {
                "type": "integer",
                "minimum": 0
              },
              "stopTimeMs": {
                "type": "integer",
                "minimum": 0
              },
              "type": {
                "const": "object"
              }
            },
            "required": [
              "startTimeMs",
              "stopTimeMs"
            ]
          }
        ]
      }
    }
  },
  "anyOf": [
    {
      "$ref": "master.json#/definitions/header"
    }
  ],
  "allOf": [
    {
      "properties": {
        "validationContracts": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "object"
          }
        }
      }
    }
  ]
}
