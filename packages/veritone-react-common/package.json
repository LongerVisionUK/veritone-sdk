{
  "name": "veritone-react-common",
  "version": "1.1.0",
  "main": "dist/bundle.js",
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "downshift": "^2.0.10",
    "faker": "^4.1.0",
    "immutability-helper": "^2.5.1",
    "leaflet": "^1.3.1",
    "leaflet-draw": "^1.0.2",
    "leaflet-geosearch": "^2.6.0",
    "lodash": "^4.17.4",
    "memoize-one": "^5.0.4",
    "mime-types": "^2.1.17",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "pluralize": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dnd-test-backend": "^2.5.4",
    "react-dom": "^16.8.6",
    "react-infinite-calendar": "^2.3.1",
    "react-modal": "^3.1.2",
    "react-range": "^1.0.5",
    "react-redux": "^5.0.6",
    "react-rnd": "^9.2.0",
    "recompose": "^0.30.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-form-material-ui": "^5.0.0-beta.1",
    "rxjs": "^5.5.6",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "start-storybook -p 9000 -c .storybook",
    "format": "prettier --write \"./src/**/*.{js,scss}\"",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress",
    "build:watch": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --watch",
    "build:storybook": "build-storybook -c .storybook -o docs-static",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "npm run lint:js; npm run lint:styles",
    "lint:js": "eslint --ext .js src/",
    "lint:styles": "stylelint \"src/**/*.{scss, css}\"",
    "prepublishOnly": "npm run test && npm run lint && npm run build"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "3.3.0-alpha.4",
    "@storybook/react": "v3.3.0-alpha.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^22.0.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2016": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "css-to-string-loader": "^0.1.3",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-lodash": "^2.4.5",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^4.0.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.0",
    "lint-staged": "^10.0.1",
    "node-sass": "^4.9.4",
    "prettier": "^1.19.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.18.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.6.2",
    "veritone-dev-env": "^1.0.2",
    "webpack": "^3.6.0"
  },
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^helpers(.*)$": "<rootDir>/src/helpers$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/testSuitePolyfills.js",
      "<rootDir>/test/configureEnzyme.js"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
