{
  "env": {
    "test": {
      "presets": ["env", "react"],
      "plugins": [
        [
          "module-resolver",
          {
            "root": "./src",
            "alias": {
              "helpers": "./src/helpers",
              "modules": "./src/modules",
              "util": "./src/util"
            }
          }
        ],
        "transform-export-extensions",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread",
        "lodash"
      ]
    },

    "production": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": ["> 5%"]
            }
          }
        ],
        "react"
      ],
      "plugins": [
        "external-helpers",
        [
          "module-resolver",
          {
            "alias": {
              "helpers": "./src/helpers",
              "modules": "./src/modules",
              "util": "./src/util",
              "lodash": "lodash-es"
            }
          }
        ],
        "transform-export-extensions",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread",
        "lodash"
      ]
    }
  }
}
